# Backend API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_WS_URL=ws://localhost:3001

# Authentication
VITE_AUTH_PROVIDER=clerk
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# AI/RAG Services (Client-side keys - use with caution)
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_PINECONE_API_KEY=your_pinecone_api_key_here
VITE_PINECONE_ENVIRONMENT=your_pinecone_environment_here

# External Integrations
VITE_LMS_PROVIDER=encompass
VITE_LMS_BASE_URL=https://api.encompass.com
VITE_EPA_API_BASE_URL=https://api.epa.gov

# Feature Flags
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_LMS_INTEGRATION=true
VITE_ENABLE_BATCH_PROCESSING=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# Performance Settings
VITE_ENABLE_SOURCE_MAPS=true
VITE_ENABLE_BUNDLE_ANALYZER=false

# Security Settings (Development only)
VITE_DISABLE_SSL_VERIFY=false
VITE_ALLOW_UNSAFE_INLINE=false

# Application Settings
VITE_APP_NAME=Financed Emissions Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=PCAF-compliant financed emissions tracking platform

# Monitoring and Analytics
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_ENABLE_ERROR_REPORTING=true
VITE_SENTRY_DSN=your_sentry_dsn_here